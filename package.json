{
  "name": "eth-node-status-page",
  "version": "0.1.0",
  "description": "Ethereum Node Status page",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 3000",
    "startPerf": "NODE_ENV=perf-debug npm start",
    "startTest": "NODE_ENV=tests npm start",
    "webpack": "webpack",
    "commit": "commit-wizard",
    "coveralls": "npm run testCoverage && coveralls < coverage/lcov.info",
    "lint": "eslint --ignore-path .gitignore .",
    "ci": "npm run lint & npm run testOnce & npm run nightwatchCI",
    "test": "npm run testOnce -- -w",
    "testCoverage": "istanbul cover _mocha -- 'client/**/*.spec.js'",
    "testOnce": "mocha 'client/**/*.spec.js'",
    "nightwatch": "NODE_ENV=tests nightwatch",
    "nightwatchCI": "npm run nightwatch  -- -e saucelabs",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack --progress --colors && cp Cargo.toml static",
    "clean": "rm static -rf || true",
    "prezip": "npm run build",
    "zip": "cd static && zip ../status.zip * -r",
    "genRpc": "./rpc-docs/index.js"
  },
  "license": "GPL-3.0",
  "author": "Ethcore dev team",
  "repository": {
    "type": "git",
    "url": "https://github.com/ethcore/eth-node-status-page.git"
  },
  "bugs": {
    "url": "https://github.com/ethcore/eth-node-status-page/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.5.2",
    "babel-eslint": "6.0.2",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "6.9.0",
    "babel-runtime": "^6.5.0",
    "chai": "3.5.0",
    "chai-enzyme": "^0.4.2",
    "chromedriver": "2.21.2",
    "classnames": "^2.2.3",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.9.0",
    "eslint-config-semistandard": "^6.0.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-react": "^2.4.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-standard": "^1.3.2",
    "file-loader": "^0.8.5",
    "ignore-styles": "^2.0.0",
    "istanbul": "^1.0.0-alpha",
    "jsdom": "^9.0.0",
    "json-loader": "^0.5.4",
    "karma-babel-preprocessor": "6.0.1",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.4.5",
    "mock-local-storage": "1.0.2",
    "nightwatch": "0.8.18",
    "postcss-loader": "^0.8.1",
    "pre-git": "^3.8.1",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.0.2",
    "react-dom": "^15.0.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "rucksack-css": "^0.8.5",
    "selenium-server-standalone-jar": "2.53.0",
    "selenium-webdriver": "2.53.1",
    "sinon": "1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.7.1",
    "why-did-you-update": "0.0.8",
    "xhr-interceptor": "^1.3.0"
  },
  "dependencies": {
    "bignumber.js": "2.3.0",
    "browser-request": "0.3.3",
    "bytes": "2.3.0",
    "dapp-styles": "^0.2.2",
    "format-json": "1.0.3",
    "format-number": "2.0.1",
    "history": "2.1.1",
    "lodash": "4.11.1",
    "marked": "^0.3.5",
    "material-ui": "0.15.0-beta.2",
    "react-addons-css-transition-group": "15.0.2",
    "react-copy-to-clipboard": "4.1.0",
    "react-tap-event-plugin": "1.0.0",
    "rlp": "2.0.0",
    "store": "1.3.20",
    "web3": "^0.15.3"
  },
  "config": {
    "pre-git": {
      "pre-commit": [
        "./scripts/check-master.sh"
      ],
      "pre-push": [
        "npm run lint",
        "npm run testOnce"
      ],
      "post-commit": [],
      "post-checkout": [],
      "post-merge": []
    }
  }
}
